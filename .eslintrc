{
    "env": {
       "browser": true,
       "node": true,
       "es6": true,
       "commonjs": true
    },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "extends": [ "eslint:recommended", "plugin:react/recommended" ],
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "^16.4.2"
    }
  },
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn"
    }
}
